//Task
//We need a function that can transform a number (integer) into a string.

//Answer
function numberToString(num) {
return num.toString()
}

//Othes' answers:

function numberToString(num) {
  // Return a string of the number here!
  return String(num);
}

function numberToString(num) {
  return ''+num;
}

const numberToString = num => `${num}`;
